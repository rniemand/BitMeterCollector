{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "BitMeter": {
    "Servers": [
      {
        "Enabled": true,
        "UseHttps": false,
        "IPAddress": "127.0.0.1",
        "Port": 9876,
        "ServerName": "Dev"
      }
    ]
  },
  "Rn.Metrics": {
    "enabled": true,
    "application": "BitmeterCollector",
    "template": "bitmeter/{measurement}",
    "environment": "development"
  },
  "Rn.Metrics.Rabbit": {
    "enabled": true,
    "username": "bob",
    "password": "password",
    "virtualHost": "/",
    "host": "192.168.0.60",
    "port": 5672,
    "exchange": "amq.topic",
    "routingKey": "metrics.bitmeter_collector",
    "backOffTimeSec": 15,
    "coolDownTimeSec": 300,
    "coolDownThreshold": 3,
    "maxCoolDownRuns": 0
  }
}
